{
	"AppName": "unseal",
	"ArtifactsDest": "downloads",
	"Tasks": [
		"interpolate-source",
		"xc",
		"archive-zip",
		"archive-tar-gz",
    "publish-github"
	],
	"BuildConstraints": "linux,amd64 windows,amd64 darwin,amd64",
	"ResourcesInclude": "README*",
	"PackageVersion": "0.3.1",
	"TaskSettings": {
		"publish-github": {
			"apikey": "",
      "body": "See: https://github.com/jaxxstorm/unseal/blog/master/CHANGELOG.md",
      "include": "*.zip,*.tar.gz",
      "outputFormat": "by-file-extension",
      "owner": "jaxxstorm",
      "repository": "unseal"
			}
		},
	"ConfigVersion": "0.9"
}
